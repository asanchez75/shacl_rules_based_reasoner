  mcf:InsertOn example:Thing_B ;
  rdfs:subClassOf owl:Thing ;
.
example:Thing_D
  rdf:type owl:Class ;
  rdfs:subClassOf example:Thing_A ;
.
example:Thing_E
  rdf:type owl:Class ;
  mcf:IsInvolvedIn example:Thing_B_1 ;
  rdfs:subClassOf example:Thing_D ;
.
example:custom1
  rdf:type rdfs:Class ;
  rdf:type sh:NodeShape ;
  rdfs:subClassOf rdfs:Resource ;
  sh:rule [
      rdf:type sh:SPARQLRule ;
      sh:construct """
      PREFIX mcf: <http://www.mycorporisfabrica.org/ontology/mcf.owl#>
      PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
      CONSTRUCT {
        $this mcf:InsertOn ?b.
      }
      WHERE {
        $this rdfs:subClassOf ?c.
        ?c mcf:InsertOn ?b.
      }
      """ ;
    ] ;
  sh:targetClass owl:Class ;
  sh:targetSubjectsOf rdfs:subClassOf ;
.

